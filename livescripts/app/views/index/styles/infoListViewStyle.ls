require! ["/app/styles/colors"]
base-font = {
	font-size: "30px"
}
module.exports = {
	#page-view
	info-list-view:
		background-color: "white"
		background-image: ""
		top: "120px"
		bottom: "120px"
		width: "100%"

	#scroll-container	
	scroll-container-view:
		height: "100%"
		width: "100%"
